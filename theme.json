				{
					"color": "#a11477",
					"name": "Magenta",
					"slug": "primary"
				},
				{
					"color": "#e13661",
					"name": "Pink",
					"slug": "secondary"
				},
				{
					"color": "#ff6f4b",
					"name": "Orange",
					"slug": "tertiary"
