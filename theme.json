			"defaultPalette": false,
			"duotone": [
				{
					"colors": [
						"var(--wp--preset--color--light)",
						"var(--wp--preset--color--contrast)"
					],
					"name": "Light-Dark",
					"slug": "light-contrast"
				},
				{
					"colors": [
						"var(--wp--preset--color--contrast)",
						"var(--wp--preset--color--light)"
					],
					"name": "Dark-Light",
					"slug": "contrast-light"
				},
				{
					"colors": [
						"var(--wp--preset--color--primary)",
						"var(--wp--preset--color--secondary)"
